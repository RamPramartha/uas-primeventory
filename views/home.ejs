    </header>
    <div class="banner" style="background-image: url('/assets/LoginCar.jpg'); height: 350px;">
        <div class="konten-banner">
            <h1 class="text-center text-white font-semibold" style="font-size: 40px; padding-top: 125px;">Selamat Datang!</h1>
            <p class="text-center text-white font-medium mt-0" style="font-size: 40px;">Ingin Meminjam Ruangan?</p>
        </div>
    </div>
    <main class="mt-10 ml-4 mr-4">
        <div class="lab">
            <h2 class="font-medium" style="font-size: 25px;">Lab</h2>
            <hr style="border: 0.5px solid gray;">

            <!-- Lab -->
            <div class="flex flex-wrap gap-5 justify-center">
                <% lab.forEach(function(l) { %> 
                    <div class="card mt-5 rounded-md" style="width: 350px; height: auto; background: #003764; box-shadow: 2px 2px 6px;">
                        <div style="background-image:url('/uploads/<%= l.foto_ruangan %> ') ; height: 235px; background-size: cover;"></div>
                        <div class="konten-card p-4">
                            <h2 class="text-lg text-white font-bold"><%= l.nama_ruangan %></h2>
                            <p class="text-sm text-white"> <%= l.deskripsi_ruangan %></p>
                            <a href="/peminjaman/<%= l.kode_ruangan %>">
                                <button type="" class="px-3 py-2 bg-white rounded-lg mt-7" style="font: #003764; width: 100%;">PINJAM SEKARANG</button>
                            </a>
                        </div>
                    </div>
                <% }); %>
    
                
            </div>
        </div>
        <div class="kelas mt-5">
            <h2 class="font-medium" style="font-size: 25px;">Kelas</h2>
            <hr style="border: 0.5px solid gray;">

            <!-- Kelas -->
            <div class="flex flex-wrap gap-5 justify-center">
               
                <div class="flex flex-wrap gap-5 justify-center">
                    <% kelas.forEach(function(k) { %> 
                        <div class="card mt-5 rounded-md" style="width: 350px; height: auto; background: #003764; box-shadow: 2px 2px 6px;">
                            <div style="background-image:url('/uploads/<%= k.foto_ruangan %> ') ; height: 235px; background-size: cover;"></div>
                            <div class="konten-card p-4">
                                <h2 class="text-lg text-white font-bold"><%= k.nama_ruangan %></h2>
                                <p class="text-sm text-white"> <%= k.deskripsi_ruangan %></p>
                                <a href="/peminjaman/<%= k.kode_ruangan %>">
                                    <button type="" class="px-3 py-2 bg-white rounded-lg mt-7" style="font: #003764; width: 100%;">PINJAM SEKARANG</button>
                                </a>
                            </div>
                        </div>
                    <% }); %>  
                </div>

            </div>
            </div>
        </div>

        <!-- Lainnya -->
        <div class="lainnya mt-5">
            <h2 class="font-medium" style="font-size: 25px;">Lainnya</h2>
            <hr style="border: 0.5px solid gray;">

            <div class="flex flex-wrap gap-5 justify-center">

                <div class="flex flex-wrap gap-5 justify-center">
                    <% lain.forEach(function(la) { %> 
                        <div class="card mt-5 rounded-md" style="width: 350px; height: auto; background: #003764; box-shadow: 2px 2px 6px;">
                            <div style="background-image:url('/uploads/<%= la.foto_ruangan %> ') ; height: 235px; background-size: cover;"></div>
                            <div class="konten-card p-4">
                                <h2 class="text-lg text-white font-bold"><%= la.nama_ruangan %></h2>
                                <p class="text-sm text-white"> <%= la.deskripsi_ruangan %></p>
                                <a href="/peminjaman/<%= la.kode_ruangan %>">
                                    <button type="" class="px-3 py-2 bg-white rounded-lg mt-7" style="font: #003764; width: 100%;">PINJAM SEKARANG</button>
                                </a>
                            </div>
                        </div>
                    <% }); %>  
                </div>
  
              
  
              
              

            

            
         
  
             
  
              
    </main>
    <footer class=" bg-blue-950 mt-8" style="height: 40vh;">

    </footer>
    <script>
        // Burger menus
        document.addEventListener('DOMContentLoaded', function() {
            // open
            const burger = document.querySelectorAll('.navbar-burger');
            const menu = document.querySelectorAll('.navbar-menu');
        
            if (burger.length && menu.length) {
                for (var i = 0; i < burger.length; i++) {
                    burger[i].addEventListener('click', function() {
                        for (var j = 0; j < menu.length; j++) {
                            menu[j].classList.toggle('hidden');
                        }
                    });
                }
            }
        
            // close
            const close = document.querySelectorAll('.navbar-close');
            const backdrop = document.querySelectorAll('.navbar-backdrop');
        
            if (close.length) {
                for (var i = 0; i < close.length; i++) {
                    close[i].addEventListener('click', function() {
                        for (var j = 0; j < menu.length; j++) {
                            menu[j].classList.toggle('hidden');
                        }
                    });
                }
            }
        
            if (backdrop.length) {
                for (var i = 0; i < backdrop.length; i++) {
                    backdrop[i].addEventListener('click', function() {
                        for (var j = 0; j < menu.length; j++) {
                            menu[j].classList.toggle('hidden');
                        }
                    });
                }
            }
        });
        </script>
</body>
</html>